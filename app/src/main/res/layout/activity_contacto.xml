<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.diogo.minhaaplicacao.Loja">

    <Button
        android:id="@+id/button2"
        android:layout_width="412dp"
        android:layout_height="90dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:background="#a4c639" />


    <TextView
        android:id="@+id/textView62"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/imageView21"
        android:layout_below="@+id/button2"
        android:layout_marginStart="10dp"
        android:layout_marginTop="19dp"
        android:text="Tem alguma dúvida?"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/textView63"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView62"
        android:layout_below="@+id/textView62"
        android:layout_marginTop="10dp"
        android:text="Contacte o nosso suporte e reporte o seu problema."
        android:textSize="18sp" />

    <TextView
        android:id="@+id/textView64"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView63"
        android:layout_below="@+id/textView63"
        android:layout_marginTop="30dp"
        android:text="Enviar mensagem"
        android:textSize="22sp"
        android:textStyle="bold" />

    <EditText
        android:id="@+id/editText18"
        android:layout_width="337dp"
        android:layout_height="50dp"
        android:layout_alignStart="@+id/editText19"
        android:layout_below="@+id/editText19"
        android:paddingLeft="20dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/input_text2"
        android:ems="10"
        android:hint="Assunto"
        android:inputType="textPersonName" />

    <EditText
        android:id="@+id/editText19"
        android:layout_width="337dp"
        android:layout_height="50dp"
        android:layout_alignStart="@+id/textView64"
        android:layout_below="@+id/textView64"
        android:paddingLeft="20dp"
        android:layout_marginTop="23dp"
        android:background="@drawable/input_text2"
        android:ems="10"
        android:hint="@string/Email"
        android:inputType="textEmailAddress" />

    <EditText
        android:id="@+id/editText22"
        android:layout_width="337dp"
        android:layout_height="100dp"
        android:layout_alignStart="@+id/editText18"
        android:layout_below="@+id/editText18"
        android:gravity="top"
        android:background="@drawable/input_text2"
        android:paddingLeft="20dp"
        android:paddingTop="12dp"
        android:layout_marginTop="13dp"
        android:ems="10"
        android:hint="Escreva a sua mensagem"
        android:inputType="textMultiLine" />

    <TextView
        android:id="@+id/textView65"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/button10"
        android:layout_below="@+id/button10"
        android:layout_marginTop="22dp"
        android:text="Pode também contactar-nos por telefone:"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/textView66"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView65"
        android:layout_below="@+id/textView65"
        android:layout_marginTop="17dp"
        android:text="913  232  289  "
        android:textSize="20sp"
        android:textStyle="bold" />

    <Button
        android:id="@+id/button10"
        android:layout_width="106dp"
        android:layout_height="47dp"
        android:layout_alignStart="@+id/editText22"
        android:layout_below="@+id/editText22"
        android:layout_marginTop="24dp"
        android:background="@drawable/mybutton2"
        android:text="Enviar"
        android:textColor="@android:color/background_light"
        android:textSize="19sp" />

    <TextView
        android:id="@+id/textView68"
        android:layout_width="wrap_content"
        android:layout_height="30dp"
        android:layout_alignBaseline="@+id/button2"
        android:layout_alignBottom="@+id/button2"
        android:layout_alignEnd="@+id/editText19"
        android:text="Contacte-nos"
        android:textColor="@android:color/background_light"
        android:textSize="28sp"
        android:textStyle="bold" />

    <ImageView
        android:id="@+id/imageView21"
        android:layout_width="30dp"
        android:layout_height="25dp"
        android:layout_alignBottom="@+id/textView68"
        android:layout_alignStart="@+id/back7"
        android:layout_marginStart="15dp"
        android:contentDescription="@string/desc"
        android:src="@drawable/arrow"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="16dp" />

    <Button
        android:id="@+id/back7"
        android:layout_width="80dp"
        android:layout_height="55dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="10dp"
        android:layout_marginTop="10dp"
        android:alpha="0.0"
        android:onClick="ClickBack7"
        android:text="Button" />

    <ImageView
        android:id="@+id/imageView55"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_below="@+id/button2"
        android:src="@drawable/separacao" />

    <ImageView
        android:id="@+id/imageView56"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_below="@+id/imageView55"
        android:src="@drawable/separacao" />

    <ImageView
        android:id="@+id/imageView57"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_below="@+id/imageView56"
        android:src="@drawable/separacao" />

    <ImageView
        android:id="@+id/imageView58"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView62"
        android:layout_below="@+id/imageView56"
        android:src="@drawable/separacao" />

</RelativeLayout>